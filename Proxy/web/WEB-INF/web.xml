    <servlet>
        <servlet-name>Report</servlet-name>
        <servlet-class>Report.Report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailReport</servlet-name>
        <servlet-class>Report.DetailReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Record</servlet-name>
        <url-pattern>/Record</url-pattern>
