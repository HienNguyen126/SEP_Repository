    <servlet>
        <servlet-name>Tag</servlet-name>
        <servlet-class>Tag.Tag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>User.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OneRecord</servlet-name>
        <servlet-class>Record.OneRecord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRecord</servlet-name>
        <servlet-class>Record.AddRecord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditRecord</servlet-name>
        <servlet-class>Record.EditRecord</servlet-class>
